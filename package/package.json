{
  "name": "@gfazioli/mantine-parallax",
  "version": "2.0.2",
  "description": "Parallax component is a wrapper for any component that can be flipped. It is used to create cards with a parallax effect.",
  "homepage": "https://gfazioli.github.io/mantine-parallax/",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Giovambattista Fazioli <giovambattista.fazioli@gmail.com>",
  "keywords": [
    "extension",
    "mantine",
    "react",
    "typescript",
    "react-component",
    "animation",
    "parallax",
    "scroll",
    "branding"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gfazioli/mantine-parallax.git"
  },
  "workspaces": [
    "docs"
  ],
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "bugs": "https://github.com/gfazioli/mantine-parallax/issues"
}
